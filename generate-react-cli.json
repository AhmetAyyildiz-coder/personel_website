{
  "usesTypeScript": false,
  "usesCssModule": false,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/shared/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "src/core/utils/codeGenerator/templates/component/TemplateName.jsx",
        "style": "src/core/utils/codeGenerator/templates/component/TemplateName.scss"
      }
    },
    "layout": {
      "path": "src/shared/layouts",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "src/core/utils/codeGenerator/templates/layout/TemplateName.layout.jsx",
        "style": "src/core/utils/codeGenerator/templates/layout/TemplateName.layout.scss"
      }
    }
  }
}
